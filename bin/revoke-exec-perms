    -o -name "*.xsl" \
    -o -name "*.xslt" \
    -o -name "*.xslx" \
    -o -name "*.zip" \
    -o -name "*.zlib" \
    -o -name "ChangeLog" \
    -o -name "Makefile" \
    -o -name "README" \
    \) \
