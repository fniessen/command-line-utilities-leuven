#!/usr/bin/env sh

printf "Revoke exec permission for specified file extensions...\n"

find . -type f \
    \( -name "*.DTD" \
    -o -name "*.DWG" \
    -o -name "*.MF" \
    -o -name "*.PDF" \
    -o -name "*.PNG" \
    -o -name "*.R" \
    -o -name "*.SQL" \
    -o -name "*.TTF" \
    -o -name "*.TXT" \
    -o -name "*.Txt" \
    -o -name "*.XE" \
    -o -name "*.actionScriptProperties" \
    -o -name "*.acts" \
    -o -name "*.appcache" \
    -o -name "*.as" \
    -o -name "*.asa" \
    -o -name "*.asp" \
    -o -name "*.awk" \
    -o -name "*.axvw" \
    -o -name "*.bak" \
    -o -name "*.bat" \
    -o -name "*.bin" \
    -o -name "*.bmp" \
    -o -name "*.c" \
    -o -name "*.ced" \
    -o -name "*.cfg" \
    -o -name "*.checkstyle" \
    -o -name "*.class" \
    -o -name "*.classpath" \
    -o -name "*.css" \
    -o -name "*.csv" \
    -o -name "*.db" \
    -o -name "*.dflt" \
    -o -name "*.doc" \
    -o -name "*.docx" \
    -o -name "*.dtd" \
    -o -name "*.duw" \
    -o -name "*.dwg" \
    -o -name "*.el" \
    -o -name "*.emf" \
    -o -name "*.eot" \
    -o -name "*.fbprefs" \
    -o -name "*.flexProperties" \
    -o -name "*.frms" \
    -o -name "*.gemspec" \
    -o -name "*.gif" \
    -o -name "*.gitignore" \
    -o -name "*.gradle" \
    -o -name "*.htm" \
    -o -name "*.html" \
    -o -name "*.ico" \
    -o -name "*.idx" \
    -o -name "*.iml" \
    -o -name "*.ini" \
    -o -name "*.jar" \
    -o -name "*.java" \
    -o -name "*.jpeg" \
    -o -name "*.jpg" \
    -o -name "*.js" \
    -o -name "*.jsb" \
    -o -name "*.jshintignore" \
    -o -name "*.jshintrc" \
